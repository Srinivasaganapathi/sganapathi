import java.util.Scanner;

public class OddEven {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner s= new Scanner (System.in);
		int val;
		val=s.nextInt();
		if(val%2==0)
			System.out.println("Even");
		else
			System.out.println("Odd");
		
		s.close();
	}

}
